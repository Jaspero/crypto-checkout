{
  "name": "@jaspero/crypto-checkout",
  "version": "0.0.1",
  "description": "Crypto payments",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run build:example:watch\" \"npm run serve\"",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "build:example:watch": "tsc --watch --p dev/tsconfig.json",
    "clean": "rimraf dev/dist",
    "serve": "web-dev-server --node-resolve --watch --open --root-dir dev"
  },
  "author": {
    "name": "Jaspero Ltd",
    "email": "info@jaspero.co"
  },
  "license": "MIT",
  "dependencies": {
    "lit-element": "2.4.0",
    "qr-code-styling": "1.3.4"
  },
  "devDependencies": {
    "@rollup/plugin-replace": "2.4.2",
    "@web/dev-server": "0.1.11",
    "concurrently": "6.0.1",
    "deepmerge": "4.2.2",
    "rimraf": "3.0.2",
    "rollup": "2.45.1",
    "rollup-plugin-filesize": "9.1.1",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-terser": "7.0.2",
    "typescript": "4.2.4",
    "web-component-analyzer": "1.1.6"
  }
}
